#! /bin/sh

cat >out.exp <<\EOF
digraph A {
  rankdir=LR
  node [shape=circle]
  {
    node [style=invis,shape=none,label="",width=0,height=0]
    I1
    F1
  }
  I1 -> 1
  1 -> F1
  1 -> 2 [label="a"]
  2 -> 2 [label="b + c"]
  2 -> 1 [label="a + c"]
}
EOF

run 0 out.exp unit/ladybird-b 2
run 0 out.exp unit/ladybird-z 2

