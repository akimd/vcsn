%weight: zr
%include: common-weights.rat

{a}b => {a}b
{{0}a}b => \z
{{1}\e}b => b


# Check the "(* min, max)" quantifier.
({a}b)(*0) => \e
({a}b)(*1) => {a}b
({a}b)(*2) => {a}b.{a}b
({a}b)(*5) => {a}b.{a}b.{a}b.{a}b.{a}b

({a}b)(*0,0) == ({a}b)(*0)
({a}b)(*1,1) == ({a}b)(*1)
({a}b)(*2,2) == ({a}b)(*2)
({a}b)(*5,5) == ({a}b)(*5)

({a}b)(*0,1) => \e+{a}b
({a}b)(*0,2) => \e+{a}b+({a}b.{a}b)
({a}b)(*0,3) => \e+{a}b+({a}b.{a}b)+({a}b.{a}b.{a}b)

({a}b)(*,1) == ({a}b)(*0,1)
({a}b)(*,2) == ({a}b)(*0,2)
({a}b)(*,3) == ({a}b)(*0,3)

({a}b)(*1,2) => {a}b.(\e+{a}b)
({a}b)(*1,3) => {a}b.(\e+{a}b+({a}b.{a}b))

({a}b)(*2,5) => {a}b.{a}b.(\e+{a}b+({a}b.{a}b)+({a}b.{a}b.{a}b))

({a}b)(*0,) => ({a}b)*
({a}b)(*1,) => {a}b.(({a}b)*)
({a}b)(*4,) => {a}b.{a}b.{a}b.{a}b.(({a}b)*)
