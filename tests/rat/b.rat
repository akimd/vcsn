%weight: b
%include: common-weights.rat

# Check character classes.
[a] => a
[ac] => a+c
[abcd] => a+b+c+d
[abcdabcd] => a+b+c+d
[badcabcd] => a+b+c+d
[a-a] => a
[a-b] => a+b
[a-bc-d] => a+b+c+d

[b-a] ! invalid letter interval: b-a
[]    ! invalid empty letter class

[^] => a+b+c+d
[^a] => b+c+d
[^a-b] => c+d
[^a-bd] => c
[^b-a] ! invalid letter interval: b-a
[^a-d] ! invalid empty letter class
