%weight: b

# The base cases.
a  => a
\e => \e
\z => \z
a+b => (a+b)
ab => (a.b)
a.b => (a.b)
a* => (a)*

# Precedence and associativity.
(a+b)* == ((a+b)*)
a*b* == (a*)(b*)
a*+b* == ((a*)+(b*))
a+b* == (a+(b*))
a.b* == (a.(b*))
ab* == a(b*)
aa* == a(a*)
a** == (a*)*
(a+b).c+b == ((a+b).c)+b
(a+\e)*+b == ((a+\e)*)+b
(ab)* == (((a)b)*)
\e+a == (\e+a)
abc == ((ab)c)
a == (a)
(a(bc)) == a(bc)
a+b+c == (a+b)+c
a+\e == (a+\e)
\e+\e == (\e+\e)
(abc).(def) == abcdef
(a+b)+(c+d) == a+b+c+d

# Trivial identities.
\z*  => \e
a+\z => \z
a\e  => a
