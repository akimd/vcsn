#! /bin/sh

## ------------- ##
## is-complete.  ##
## ------------- ##

cat >a.gv <<\EOF
digraph {
    vcsn_context="lal_char(ab)_b"
    2 -> 2 [label="a"]
}
EOF

run 2 "false" -vcsn is-complete -A -f a.gv

cat >a.gv <<\EOF
digraph {
    vcsn_context="lal_char(ab)_b"
    2 -> 2 [label="a, b"]
}
EOF

run 2 "false" -vcsn is-complete -A -f a.gv

cat >a.gv <<\EOF
digraph {
    vcsn_context="lal_char(ab)_b"
    I0 -> 2
    2 -> 2 [label="a"]
}
EOF

run 2 "false" -vcsn is-complete -A -f a.gv

cat >a.gv <<\EOF
digraph {
    vcsn_context="lal_char(ab)_b"
    I0 -> 2
    2 -> 2 [label="a, b"]
}
EOF

run 0 "true" -vcsn is-complete -A -f a.gv
